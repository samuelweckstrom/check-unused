{
  "name": "check-unused",
  "description": "Check for unused dependencies",
  "version": "0.0.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "url": "https://github.com/samuelweckstrom/check-unused",
  "license": "MIT",
  "keywords": [
    "NPM",
    "check",
    "unused",
    "package",
    "dependency",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/samuelweckstrom/check-unused"
  },
  "bin": {
    "check-unused": "dist/index.js"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc -b",
    "test": "tsc -b && ava"
  },
  "devDependencies": {
    "@types/node": "^12.12.12",
    "ava": "^2.4.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.5.3"
  },
  "ava": {
    "files": [
      "!**/*test.d.*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  }
}
